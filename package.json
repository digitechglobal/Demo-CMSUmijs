    "@ant-design/pro-descriptions": "^1.0.26",
    "@ant-design/pro-layout": "^6.5.3",
    "@ant-design/pro-table": "^2.9.6",
    "@types/classnames": "^2.2.10",
    "@types/store": "^2.0.2",
    "@umijs/plugin-blocks": "^2.1.2",
    "@umijs/preset-ant-design-pro": "^1.2.2",
    "@umijs/preset-react": "1.x",
    "@umijs/preset-ui": "^2.1.9",
    "@umijs/test": "^3.2.3",
    "antd": "^4.6.4",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.28",
    "dotenv": "^8.2.0",
    "jodit": "^3.4.26",
    "jodit-react": "^1.0.63",
    "lint-staged": "^10.0.7",
    "lodash": "^4.17.15",
    "numeral": "^2.0.6",
    "prettier": "^1.19.1",
    "querystring": "^0.2.0",
    "react": "^16.14.0",
    "react-dom": "^16.12.0",
    "react-helmet-async": "^1.0.6",
    "react-hook-form": "^6.9.6",
    "store": "^2.0.12",
    "umi": "^3.2.9",
